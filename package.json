{
  "name": "portinari-sources",
  "version": "1.28.0",
  "description": "Portinari",
  "homepage": "https://portinari.io",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/portinariui/portinari-angular"
  },
  "scripts": {
    "ng": "ng",
    "tsc": "tsc",
    "start": "ng serve",
    "lint": "ng lint --type-check --format=stylish",
    "lint:storage": "ng lint storage --type-check --format=stylish",
    "lint:sync": "ng lint sync --type-check --format=stylish",
    "lint:ui": "ng lint ui --type-check --format=stylish",
    "lint:templates": "ng lint templates --type-check --format=stylish",
    "lint:code-editor": "ng lint code-editor --type-check --format=stylish",
    "e2e": "ng e2e",
    "test": "ng test --watch=false --code-coverage --browsers=ChromeHeadless --source-map",
    "test:storage": "ng test storage --watch=false --code-coverage --browsers=ChromeHeadless --source-map",
    "test:sync": "ng test sync --watch=false --code-coverage --browsers=ChromeHeadless --source-map",
    "test:ui": "ng test ui --watch=false --code-coverage --browsers=ChromeHeadless --source-map",
    "test:ui:browse": "ng test ui --code-coverage --browsers=Chrome --source-map",
    "test:templates": "ng test templates --watch=false --code-coverage --browsers=ChromeHeadless --source-map",
    "test:templates:browse": "ng test templates --code-coverage --browsers=Chrome --source-map",
    "test:code-editor": "ng test code-editor --watch=false --code-coverage --browsers=ChromeHeadless --source-map",
    "test:dev": "ng test --browsers=ChromeHeadless",
    "build:schematics": "npm run tsc -- -p ./projects/ui/tsconfig.schematics.json",
    "build:ui": "ng build ui --prod && gulp schematics && gulp replaceVersion && cd dist/portinari-ui && npm pack",
    "build:code-editor": "ng build code-editor --prod && gulp replaceVersion && cd dist/portinari-code-editor && npm pack",
    "build:templates": "ng build templates --prod && gulp replaceVersion && cd dist/portinari-templates && npm pack",
    "build:storage": "ng build storage --prod && gulp replaceVersion && cd dist/portinari-storage && npm pack",
    "build:sync": "ng build sync --prod && gulp replaceVersion && cd dist/portinari-sync && npm pack",
    "build": "npm run build:ui && npm run build:templates && npm run build:storage && npm run build:sync && npm run build:code-editor",
    "build:lite": "ng build ui && ng build storage && ng build sync && ng build templates && ng build code-editor",
    "release": "standard-version",
    "sonarqube": "gulp sonarqube",
    "format:fix": "pretty-quick --staged",
    "format:check": "prettier --check --loglevel error \"**/*{.ts,.js,.json,.css,.scss,.html}\"",
    "format:all": "prettier --write \"**/*{.ts,.js,.json,.css,.scss,.html}\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.4",
    "@angular/common": "~9.0.4",
    "@angular/compiler": "~9.0.4",
    "@angular/core": "~9.0.4",
    "@angular/forms": "~9.0.4",
    "@angular/platform-browser": "~9.0.4",
    "@angular/platform-browser-dynamic": "~9.0.4",
    "@angular/router": "~9.0.4",
    "@ionic-native/core": "5.21.5",
    "@ionic-native/network": "5.21.5",
    "@portinari/style": "1.28.0",
    "core-js": "3.1.4",
    "custom-idle-queue": "2.1.2",
    "http-status-codes": "^1.4.0",
    "localforage": "1.4.0",
    "lokijs": "1.5.7",
    "monaco-editor": "0.15.6",
    "rxjs": "~6.5.4",
    "rxjs-compat": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.4",
    "@angular-devkit/build-ng-packagr": "~0.900.4",
    "@angular/cli": "~9.0.4",
    "@angular/compiler-cli": "~9.0.4",
    "@angular/language-service": "~9.0.4",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-angular": "^8.3.4",
    "@commitlint/travis-cli": "^8.3.5",
    "@portinari/tslint": "1.0.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/localforage": "0.0.34",
    "@types/node": "^12.11.1",
    "@types/systemjs": "^0.20.6",
    "codelyzer": "^5.1.2",
    "del": "^3.0.0",
    "gulp": "^4.0.2",
    "gulp-run": "^1.7.1",
    "husky": "^4.2.3",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^9.0.0",
    "prettier": "1.19.1",
    "pretty-quick": "^2.0.1",
    "protractor": "~5.4.0",
    "sonarqube-scanner": "^2.5.0",
    "standard-version": "^7.1.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.1.0",
    "typemoq": "^2.1.0",
    "typescript": "~3.7.5"
  },
  "standard-version": {
    "skip": {
      "commit": true,
      "tag": true
    }
  }
}
