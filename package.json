{
  "name": "portinari-sources",
  "version": "1.1.1",
  "description": "Portinari",
  "homepage": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/portinariui/portinari-angular"
  },
  "scripts": {
    "ng": "ng",
    "tsc": "tsc",
    "start": "ng serve",
    "lint": "ng lint --type-check --format=stylish",
    "lint:storage": "ng lint storage --type-check --format=stylish",
    "lint:sync": "ng lint sync --type-check --format=stylish",
    "lint:ui": "ng lint ui --type-check --format=stylish",
    "lint:templates": "ng lint templates --type-check --format=stylish",
    "lint:code-editor": "ng lint code-editor --type-check --format=stylish",
    "e2e": "ng e2e",
    "test": "ng test --watch=false --code-coverage --browsers=ChromeHeadless --source-map",
    "test:storage": "ng test storage --watch=false --code-coverage --browsers=ChromeHeadless --source-map",
    "test:sync": "ng test sync --watch=false --code-coverage --browsers=ChromeHeadless --source-map",
    "test:ui": "ng test ui --watch=false --code-coverage --browsers=ChromeHeadless --source-map",
    "test:templates": "ng test templates --watch=false --code-coverage --browsers=ChromeHeadless --source-map",
    "test:code-editor": "ng test code-editor --watch=false --code-coverage --browsers=ChromeHeadless --source-map",
    "test:dev": "ng test --browsers=ChromeHeadless",
    "build:schematics": "npm run tsc -- -p ./projects/ui/tsconfig.schematics.json",
    "build:ui": "ng build ui && gulp schematics && gulp replaceVersion && cd dist/portinari-ui && npm pack",
    "build:code-editor": "ng build code-editor && gulp replaceVersion && cd dist/portinari-code-editor && npm pack",
    "build:templates": "ng build templates && gulp replaceVersion && cd dist/portinari-templates && npm pack",
    "build:storage": "ng build storage && gulp replaceVersion && cd dist/portinari-storage && npm pack",
    "build:sync": "ng build sync && gulp replaceVersion && cd dist/portinari-sync && npm pack",
    "build": "npm run build:ui && npm run build:templates && npm run build:storage && npm run build:sync && npm run build:code-editor",
    "build:lite": "ng build ui && ng build storage && ng build sync && ng build templates && ng build code-editor",
    "release": "standard-version",
    "sonarqube": "gulp sonarqube"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.0.0",
    "@angular/common": "~8.0.0",
    "@angular/compiler": "~8.0.0",
    "@angular/core": "~8.0.0",
    "@angular/forms": "~8.0.0",
    "@angular/platform-browser": "~8.0.0",
    "@angular/platform-browser-dynamic": "~8.0.0",
    "@angular/router": "~8.0.0",
    "@ionic-native/core": "4.18.0",
    "@ionic-native/network": "4.18.0",
    "@portinari/style": "1.1.1",
    "custom-idle-queue": "2.1.2",
    "http-status-codes": "^1.3.0",
    "localforage": "1.4.0",
    "monaco-editor": "0.15.6",
    "rxjs": "~6.4.0",
    "rxjs-compat": "~6.4.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0",
    "@angular-devkit/build-ng-packagr": "~0.800.0",
    "@angular/cli": "~8.0.1",
    "@angular/compiler-cli": "~8.0.0",
    "@angular/language-service": "~8.0.0",
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-angular": "^8.0.0",
    "@portinari/tslint": "1.0.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/localforage": "0.0.34",
    "@types/node": "~8.9.4",
    "@types/systemjs": "^0.20.6",
    "codelyzer": "^5.0.0",
    "del": "^3.0.0",
    "gulp": "^4.0.2",
    "gulp-run": "^1.7.1",
    "husky": "^3.0.0",
    "jasmine": "^3.3.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^5.1.0",
    "protractor": "~5.4.0",
    "sonarqube-scanner": "^2.4.1",
    "standard-version": "^6.0.1",
    "ts-node": "~7.0.0",
    "tsickle": "^0.35.0",
    "tslint": "~5.15.0",
    "typemoq": "^2.1.0",
    "typescript": "~3.4.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "standard-version": {
    "skip": {
      "commit": true,
      "tag": true
    }
  }
}
