{
  "name": "po-ui-sources",
  "version": "2.8.0",
  "description": "PO UI",
  "homepage": "https://po-ui.io",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/po-ui/po-angular"
  },
  "scripts": {
    "ng": "ng",
    "tsc": "tsc",
    "start": "ng serve",
    "lint": "ng lint --type-check --format=stylish",
    "lint:storage": "ng lint storage --type-check --format=stylish",
    "lint:sync": "ng lint sync --type-check --format=stylish",
    "lint:ui": "ng lint ui --type-check --format=stylish",
    "lint:templates": "ng lint templates --type-check --format=stylish",
    "lint:code-editor": "ng lint code-editor --type-check --format=stylish",
    "e2e": "ng e2e",
    "test": "ng test --watch=false --code-coverage --browsers=ChromeHeadless --source-map",
    "test:storage": "ng test storage --watch=false --code-coverage --browsers=ChromeHeadless --source-map",
    "test:sync": "ng test sync --watch=false --code-coverage --browsers=ChromeHeadless --source-map",
    "test:sync:browse": "ng test sync --code-coverage --browsers=Chrome --source-map",
    "test:ui": "ng test ui --watch=false --code-coverage --browsers=ChromeHeadless --source-map",
    "test:ui:browse": "ng test ui --code-coverage --browsers=Chrome --source-map",
    "test:templates": "ng test templates --watch=false --code-coverage --browsers=ChromeHeadless --source-map",
    "test:templates:browse": "ng test templates --code-coverage --browsers=Chrome --source-map",
    "test:code-editor": "ng test code-editor --watch=false --code-coverage --browsers=ChromeHeadless --source-map",
    "test:code-editor:browse": "ng test code-editor --code-coverage --browsers=Chrome --source-map",
    "test:dev": "ng test --browsers=ChromeHeadless",
    "build:schematics": "cd ./projects/schematics && npm run build",
    "build:ui:schematics": "npm run tsc -- -p ./projects/ui/tsconfig.schematics.json",
    "build:templates:schematics": "npm run tsc -- -p ./projects/templates/tsconfig.schematics.json",
    "build:sync:schematics": "npm run tsc -- -p ./projects/sync/tsconfig.schematics.json",
    "build:storage:schematics": "npm run tsc -- -p ./projects/storage/tsconfig.schematics.json",
    "build:code-editor:schematics": "npm run tsc -- -p ./projects/code-editor/tsconfig.schematics.json",
    "build:ui": "ng build ui --prod && gulp uiSchematics && gulp replaceVersion && cd dist/ng-components && npm pack",
    "build:code-editor": "ng build code-editor --prod && gulp codeEditorSchematics && gulp replaceVersion && cd dist/ng-code-editor && npm pack",
    "build:templates": "ng build templates --prod && gulp templatesSchematics && gulp replaceVersion && cd dist/ng-templates && npm pack",
    "build:storage": "ng build storage --prod && gulp storageSchematics && gulp replaceVersion && cd dist/ng-storage && npm pack",
    "build:sync": "ng build sync --prod && gulp syncSchematics && gulp replaceVersion && cd dist/ng-sync && npm pack",
    "build": "npm run build:schematics && npm run build:ui && npm run build:templates && npm run build:storage && npm run build:sync && npm run build:code-editor",
    "build:lite": "ng build ui && ng build storage && ng build sync && ng build templates && ng build code-editor",
    "release": "standard-version",
    "sonarqube": "gulp sonarqube",
    "format:fix": "pretty-quick --staged",
    "format:check": "prettier --check --loglevel error \"**/*{.ts,.js,.json,.css,.scss,.html}\"",
    "format:all": "prettier --write \"**/*{.ts,.js,.json,.css,.scss,.html}\"",
    "publish:ui:local": "npm publish dist/ng-components --registry http://localhost:4873",
    "publish:templates:local": "npm publish dist/ng-templates --registry http://localhost:4873",
    "publish:sync:local": "npm publish dist/ng-sync --registry http://localhost:4873",
    "publish:storage:local": "npm publish dist/ng-storage --registry http://localhost:4873",
    "publish:code-editor:local": "npm publish dist/ng-code-editor --registry http://localhost:4873",
    "publish:schematics:local": "npm publish dist/ng-schematics --registry http://localhost:4873",
    "publish:local": "npm run publish:schematics:local && npm run publish:ui:local && npm run publish:templates:local && npm run publish:code-editor:local && npm run publish:sync:local && npm run publish:storage:local"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.0",
    "@angular/common": "~9.1.0",
    "@angular/compiler": "~9.1.0",
    "@angular/core": "~9.1.0",
    "@angular/forms": "~9.1.0",
    "@angular/platform-browser": "~9.1.0",
    "@angular/platform-browser-dynamic": "~9.1.0",
    "@angular/router": "~9.1.0",
    "@ionic-native/core": "5.23.0",
    "@ionic-native/network": "5.23.0",
    "@po-ui/style": "2.8.0",
    "core-js": "3.6.4",
    "custom-idle-queue": "2.1.2",
    "http-status-codes": "^1.4.0",
    "localforage": "1.4.0",
    "lokijs": "1.5.8",
    "monaco-editor": "0.20.0",
    "rxjs": "~6.5.4",
    "rxjs-compat": "~6.5.4",
    "tslib": "^1.11.1",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.901.0",
    "@angular-devkit/build-ng-packagr": "^0.901.0",
    "@angular/cli": "~9.1.0",
    "@angular/compiler-cli": "~9.1.0",
    "@angular/language-service": "~9.1.0",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-angular": "^8.3.4",
    "@commitlint/travis-cli": "^8.3.5",
    "@po-ui/ng-tslint": "2.0.0",
    "@types/jasmine": "~3.5.10",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^13.13.4",
    "@types/systemjs": "^6.1.0",
    "codelyzer": "^5.2.2",
    "del": "^5.1.0",
    "gulp": "^4.0.2",
    "gulp-run": "^1.7.1",
    "husky": "^4.2.3",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.1",
    "karma": "^5.0.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "~3.1.1",
    "karma-jasmine-html-reporter": "^1.5.3",
    "ng-packagr": "^9.1.0",
    "prettier": "2.0.2",
    "pretty-quick": "^2.0.1",
    "protractor": "~5.4.4",
    "rollup": "^1.20.0",
    "sonarqube-scanner": "^2.6.0",
    "standard-version": "^7.1.0",
    "ts-node": "~8.8.1",
    "tsickle": "^0.38.1",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typemoq": "^2.1.0",
    "typescript": "~3.8.3"
  },
  "standard-version": {
    "skip": {
      "commit": true,
      "tag": true
    }
  }
}
